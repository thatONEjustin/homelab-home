{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "allowJs": true,
        "baseUrl": ".",
        "paths": {
            "@layouts/*": [
                "src/layouts/*"
            ],
            "@components/*": [
                "src/components/*"
            ],
            "@styles/*": [
                "src/styles/*"
            ],
            "@js/*": [
                "src/js/*"
            ],
            "@img/*": [
                "src/img/*"
            ],
            "@data/*": [
                "src/js/data/*"
            ]
        }
    }
}
