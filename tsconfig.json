{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
      "baseUrl": ".", 
      "paths": {
          "@layouts/*": [
              "src/layouts/*"
          ],
          "@components/*": [
              "src/components/*"
          ],
          "@typography/*": [
              "src/components/typography/*"
          ],
          "@styles/*": [
              "src/styles/*"
          ]
      }
  }
}
